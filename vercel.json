{
  "version": 2,
  "buildCommand": "pnpm run build:tourii-backend:vercel",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/tourii-backend/src/main.ts",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS"
      ]
    }
  ]
}