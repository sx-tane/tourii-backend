                        "required": [
                          "storyId",
                          "touristSpotId",
                          "chapterNumber",
                          "chapterTitle",
                          "chapterDesc",
                          "chapterImage",
                          "characterNameList",
                          "realWorldImage",
                          "chapterVideoUrl",
                          "chapterVideoMobileUrl",
                          "chapterPdfUrl",
                          "isUnlocked",
