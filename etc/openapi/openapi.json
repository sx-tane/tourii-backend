                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "storyId": {
                            "description": "Unique identifier for the story",
                            "type": "string"
                          },
                          "touristSpotId": {
                            "description": "Unique identifier for the tourist spot",
                            "type": "string"
